{
  "name": "suprim-ui",
  "version": "1.2.3",
  "description": "Modern and minimalist UI library, originating from Suprim's design.",
  "main": "dist/index.js",
  "module": "build/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "start": "react-scripts start",
    "prettier": "prettier --write .",
    "test:lint": "eslint src/components/**/*.{js,ts,tsx}",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom --coverage --collectCoverageFrom=src/components/**/*.{js,ts,tsx}",
    "test:watch": "react-scripts test --env=jsdom --coverage --collectCoverageFrom=src/components/**/*.{js,ts,tsx}",
    "test:build": "run-s build",
    "test": "run-s test:unit test:lint  test:build",
    "prebuild": "rimraf dist build",
    "build:types": "tsc --project tsconfig.production.json",
    "build:compile": "rollup -c",
    "build:esm": "cross-env NODE_ENV=production babel src/components  --extensions \".js,.ts,.tsx\" --out-dir build --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__",
    "build": "run-s build:esm  build:types build:compile",
    "prepare": "npm run build"
  },
  "homepage": "https://suprim.org.np",
  "keywords": [
    "Suprim",
    "Suprim Ui",
    "Components",
    "React Ui",
    "Nextjs Ui"
  ],
  "author": "Suprim Golay",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/runtime": "^7.9.6",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-ts-lambdas": "^1.2.0",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "next": "^9.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.18.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "styled-components": "^5.2.0",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "styled-components": "^5.2.0"
  },
  "files": [
    "/dist",
    "/build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/suprim12/suprim-ui"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
